{
  "name": "lithic-card-manager",
  "private": true,
  "version": "0.0.0",
  "description": "App to controll subscriptions using lithic virtual cards and connect with data from apps that rate or manage subscription services. Made to be a companion for What2Watchlist.com.",
  "productName": "Sub-Troll",
  "author": "david-bfg <david.1.bfg@gmail.com>",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all -p server:web server:api",
    "server:web": "vite",
    "server:api": "node api/index.js",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "simpleddp": "^2.2.4",
    "simpleddp-plugin-login": "^4.0.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.0"
  }
}
